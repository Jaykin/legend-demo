{
  "tags": {
    "allowUnknownTags": true, // 允许使用自定义tag
    "dictionaries": ["jsdoc","closure"] // 定义tag集
  },
  "source": {
    "includePattern": ".+\\.js(doc)?$", // 将以.js, .jsdoc结尾的文件作为源文件
    "excludePattern": "(^|\\/|\\\\)_" // 忽略以_开头的文件夹及文件
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}