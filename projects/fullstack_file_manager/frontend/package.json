{
  "name": "ids",
  "version": "1.4.0",
  "description": "汽修资料管理后台",
  "main": "index.js",
  "keywords": [
    "react",
    "redux"
  ],
  "author": "wangkj",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "http://192.168.1.205:90/tech/fe-garage-doc-sys.git"
  },
  "scripts": {
    "dev": "bnr dev",
    "dev-local": "bnr dev --api ENV_LOCAL",
    "dev-release": "bnr dev --api ENV_RELEASE",
    "test": "bnr test",
    "test-coverage": "bnr test --coverage",
    "precommit": "lint-staged",
    "jslint": "eslint --fix src config",
    "lesslint": "lesshint src/**/*.less",
    "clean": "rimraf dist",
    "build": "npm run clean && bnr build",
    "update": "npm-check -u",
    "build-storybook": "build-storybook",
    "install-g": "bnr install-global-deps"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon ./config/server.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": "3000"
      }
    },
    "test": {
      "command": "jest --watch",
      "NODE_ENV": "test"
    },
    "build": {
      "command": "node ./config/server.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "install-global-deps": {
      "command": "npm install -g rimraf npm-check nodemon"
    }
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-import": "^1.2.1",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-remove-strict-mode": "0.0.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "better-npm-run": "0.0.15",
    "colors": "^1.1.2",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "eslint": "^4.2.0",
    "eslint-plugin-react": "^7.1.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-minifier": "^3.2.3",
    "html-webpack-plugin": "^2.24.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "react-transform-hmr": "^1.0.4",
    "style-loader": "^0.18.2",
    "webpack": "^3.2.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "antd": "^2.10.4",
    "jquery": "^1.9.1",
    "lodash": "^4.17.4",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "webuploader": "^0.1.8"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "\\.js$": "babel-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
